<?xml version="1.0"?>

<PropertyList>

	<path>Gears.ac</path>

	<animation>
		<type>material</type>
		<property-base>sim/model/livery</property-base>
		<!--
		<object-name>l_gear_13</object-name>
		<object-name>r_gear_13</object-name>
		-->
		<texture-prop>texture</texture-prop>
		<texture>Liveries/Default.png</texture>
	</animation>

<!-- Landing Gear/Visibility -->
	<animation>
		<type>select</type>
		<object-name>c_gear_01</object-name>
		<object-name>l_gear_01</object-name>
		<object-name>r_gear_01</object-name>
		<condition>
			<greater-than>
				<property>gear/gear[0]/position-norm</property>
				<value>0.001</value>
			</greater-than>
		</condition>
	</animation>

<!-- F R O N T  G E A R -->
<!-- Front Gear/Rotation -->
	<animation>
		<type>rotate</type>
		<object-name>c_gear_01</object-name>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>-100</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-100</dep>
			</entry>
		</interpolation>
		<offset-deg>0</offset-deg>
		<axis>
			<object-name>c_gear_01_axis</object-name>
		</axis>
	</animation>	
	<animation>
		<type>rotate</type>
		<object-name>c_gear_launch_bar_01</object-name>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>-100</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-100</dep>
			</entry>
		</interpolation>
		<offset-deg>0</offset-deg>
		<axis>
			<object-name>c_gear_launch_bar_01_axis</object-name>
		</axis>
	</animation>	
<!-- Fron Gear/Steering -->
	<animation>
		<type>rotate</type>
		<object-name>c_gear_03</object-name>
		<!-- <property>surface-positions/rudder-pos-norm</property> -->
        <property>controls/flight/NWS</property>
		<factor>90</factor>
		<offset-deg>0</offset-deg>
		<axis>
			<object-name>c_gear_03_axis</object-name>
		</axis>
	</animation>
<!-- Fron Gear/Compression -->
	<animation>
		<type>translate</type>
		<object-name>c_gear_04</object-name> 
		<property>gear/gear[0]/compression-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>-0.32</dep>
            </entry>
            <entry>
                <ind>2.0</ind>
                <dep>-0.32</dep>
            </entry>
        </interpolation>
		<axis>
			<object-name>c_gear_03_axis</object-name>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>c_gear_15</object-name> 
		<property>gear/gear[0]/compression-norm</property>
        <interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-30</dep>
			</entry>
        </interpolation>
		<axis>
			<object-name>c_gear_15_axis</object-name>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>c_gear_09</object-name> 
		<property>gear/gear[0]/compression-norm</property>
        <interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>70</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>70</dep>
			</entry>
        </interpolation>
		<axis>
			<object-name>c_gear_09_axis</object-name>
		</axis>
	</animation>
<!-- Fron Wheel/Spin -->
    <animation>
        <type>spin</type>
        <object-name>c_wheel_01</object-name>
        <object-name>c_wheel_02</object-name>
        <property>gear/gear[0]/rollspeed-ms</property>
        <factor>12.0</factor>
        <axis>
			<object-name>c_wheel_01_02_axis</object-name>
        </axis>
    </animation>

<!-- L E F T  G E A R -->
<!-- Left Gear Translation -->
	<animation>
		<type>translate</type>
		<object-name>l_gear_01</object-name>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>1.0</dep>
			</entry>
		</interpolation>
		<axis>
			<object-name>l_gear_011_axis</object-name>
		</axis>
	</animation>
<!-- Left Gear Rotation/Compression -->
	<animation>
		<type>rotate</type>
		<object-name>l_gear_01</object-name>
		<expression>
			<sum>
				<product>
					<property>gear/gear[1]/position-norm</property>
					<value>-100.0</value>
				</product>
				<product>
					<property>gear/gear[1]/compression-norm</property>
					<value>5.0</value>
				</product>
			</sum>
			<!--
			<interpolation>
				<entry>
					<ind>0.0</ind>
					<dep>0</dep>
				</entry>
				<entry>
					<ind>0.1</ind>
					<dep>0</dep>
				</entry>
				<entry>
					<ind>0.9</ind>
					<dep>-60</dep>
				</entry>
				<entry>
					<ind>1.0</ind>
					<dep>-100</dep>
				</entry>
			</interpolation>
			-->
		</expression>
		<offset-deg>0</offset-deg>
		<axis>
			<object-name>l_gear_01_axis</object-name>
		</axis>
	</animation>
<!-- Left Gear Compression -->
	<animation>
		<type>rotate</type>
		<object-name>l_gear_04</object-name>
		<property>gear/gear[1]/compression-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>15</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>20</dep>
			</entry>
			<entry>
				<ind>2.0</ind>
				<dep>20</dep>
			</entry>
		</interpolation>
		<axis>
			<object-name>l_gear_04_axis</object-name>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>l_gear_03</object-name>
		<property>gear/gear[1]/compression-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.3</dep>
            </entry>
            <entry>
                <ind>0.8</ind>
                <dep>0.1</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.1</dep>
            </entry>
        </interpolation>
		<axis>
			<object-name>l_gear_03_axis</object-name>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>l_gear_12</object-name>
		<property>gear/gear[1]/compression-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.3</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
		<axis>
			<object-name>l_gear_12_axis</object-name>
		</axis>
	</animation>
<!-- Left Wheel/Spin -->
    <animation>
        <type>spin</type>
        <object-name>l_wheel_01</object-name>
        <property>gear/gear[1]/rollspeed-ms</property>
        <factor>-10.0</factor>
        <axis>
			<object-name>l_wheel_01_axis</object-name>
        </axis>
    </animation>

<!-- R I G H T  G E A R -->
<!-- Right Gear Translation -->
	<animation>
		<type>translate</type>
		<object-name>r_gear_01</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-1.0</dep>
			</entry>
		</interpolation>
		<axis>
			<object-name>r_gear_011_axis</object-name>
		</axis>
	</animation>
<!-- Right Gear Rotation/Compression -->
	<animation>
		<type>rotate</type>
		<object-name>r_gear_01</object-name>
		<expression>
			<sum>
				<product>
					<property>gear/gear[2]/position-norm</property>
					<value>100.0</value>
				</product>
				<product>
					<property>gear/gear[2]/compression-norm</property>
					<value>-5.0</value>
				</product>
			</sum>
			<!--
			<interpolation>
				<entry>
					<ind>0.0</ind>
					<dep>0</dep>
				</entry>
				<entry>
					<ind>0.1</ind>
					<dep>0</dep>
				</entry>
				<entry>
					<ind>0.9</ind>
					<dep>-60</dep>
				</entry>
				<entry>
					<ind>1.0</ind>
					<dep>-100</dep>
				</entry>
			</interpolation>
			-->
		</expression>
		<offset-deg>0</offset-deg>
		<axis>
			<object-name>r_gear_01_axis</object-name>
		</axis>
	</animation>
<!-- Right Gear Compression -->
	<animation>
		<type>rotate</type>
		<object-name>r_gear_04</object-name>
		<property>gear/gear[2]/compression-norm</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>-15</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-20</dep>
			</entry>
			<entry>
				<ind>2.0</ind>
				<dep>-20</dep>
			</entry>
		</interpolation>
		<axis>
			<object-name>r_gear_04_axis</object-name>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>r_gear_03</object-name>
		<property>gear/gear[2]/compression-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.3</dep>
            </entry>
            <entry>
                <ind>0.8</ind>
                <dep>0.1</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.1</dep>
            </entry>
        </interpolation>
		<axis>
			<object-name>r_gear_03_axis</object-name>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>r_gear_12</object-name>
		<property>gear/gear[2]/compression-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.3</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
		<axis>
			<object-name>r_gear_12_axis</object-name>
		</axis>
	</animation>
<!-- Right Wheel/Spin -->
    <animation>
        <type>spin</type>
        <object-name>r_wheel_01</object-name>
        <property>gear/gear[1]/rollspeed-ms</property>
        <factor>10.0</factor>
        <axis>
			<object-name>r_wheel_01_axis</object-name>
        </axis>
    </animation>
</PropertyList>